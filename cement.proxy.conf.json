{
  "/apis/auth/*": {
    "target": "http://localhost/cementapis/",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/apis/auth": "index.php/auth"
    },
    "changeOrigin": true
  },
  "/apis/*": {
    "target": "http://localhost/cementapis/",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/apis": "index.php/apis"
    },
    "changeOrigin": true
  }
}
