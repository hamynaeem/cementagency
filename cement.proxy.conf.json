{
  "/apis/auth/*": {
    "target": "http://localhost:8000/",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/apis/auth": "index.php/auth"
    },
    "changeOrigin": true
  },
  "/apis/datatables/*": {
    "target": "http://localhost:8000/",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/apis/datatables": "api_proxy.php?REQUEST_URI=/datatables"
    },
    "changeOrigin": true
  },
  "/apis/*": {
    "target": "http://localhost:8000/",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/apis": "api_proxy.php?REQUEST_URI=/apis"
    },
    "changeOrigin": true
  },
  "/datatables/*": {
    "target": "http://localhost:8000/",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/datatables": "api_proxy.php?REQUEST_URI=/datatables"
    },
    "changeOrigin": true
  }
}
